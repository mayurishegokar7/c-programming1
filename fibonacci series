// fibonacci series
# include <stdio.h>
int main()
{
    int a=0,b=1,c,i,n;
    printf("enter number:");
    scanf("%d",& n);
    printf("fibonacci series\no\n1\n");
    for (i=1;i<=n-2;i++){
        c=a+b;
        if(c>n){
            break;
        }
        printf("%d\n",c);
        a=b;
        b=c;
    }
    return 0;
}
