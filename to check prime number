// to check prime number
# include <stdio.h>
int main()
{
    int n,i,cnt=0;
    printf("enter number:");
    scanf("%d",& n);
    if (n<=1)
       printf("%d  is not a prime number",n);
    else{
        for(i=1;i<=n;i++)
        {
        if (n%i==0)
        cnt++;
        }
        if (cnt<=2)
          printf("%d is a prime number",n);
        else
          printf("%d is not a prime number", n);
    }  
    return 0;
}
